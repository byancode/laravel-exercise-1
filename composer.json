{
    "name": "byancode/laravel-exercise-1",
    "description": "This is my package laravel-exercise-1",
    "keywords": [
        "Byancode",
        "laravel-exercise-1"
    ],
    "homepage": "https://github.com/byancode/laravel-exercise-1",
    "license": "MIT",
    "authors": [
        {
            "name": "byancode",
            "email": "byancode@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.1|^8.2",
        "laravel/framework": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Byancode\\LaravelExercise1\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Byancode\\LaravelExercise1\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Byancode\\LaravelExercise1\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
